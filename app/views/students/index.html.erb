<h1>The Students:</h1>

<ul>
  <% @students.each do |student| %>
    <li>
      <div><%= student.name %></div>
      <div><%= student.nickname %></div>
      <div><%= student.email %></div>
      <img src="<%= student.image_url %>" />
      <%= link_to 'Edit', edit_student_path(student) %>
    </li>
  <% end %>
</ul>

<%= link_to 'New Student', new_student_path %>